mpirun \
              -tag-output \
              -x LD_LIBRARY_PATH \
              -x PATH \
              -x TERM=linux \
              -x GANINMEM=/tmp/gan_data \
              -x USES3=1 \
              -x NCCL_DEBUG=INFO \
              -x NCCL_SOCKET_IFNAME=eth0 \
              -mca pml ob1 \
              -mca btl ^openib \
              python3 MPIGDriver.py bb.json train_3d.list test_3d.list \
              --tf \
              --epochs 1 \
              --features-name X \
              --labels-name y \
              --mode easgd \
              --worker-optimizer rmsprop

